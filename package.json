{
  "name": "n8n-nodes-jira-data-center",
  "version": "1.0.0",
  "description": "n8n community node for Jira Data Center with all operations and triggers",
  "keywords": [
    "n8n-community-node-package",
    "jira",
    "data-center",
    "atlassian",
    "issue-tracking",
    "project-management",
    "webhook"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sine-ai/",
  "author": {
    "name": "Samuel Kola",
    "email": "samuel.kola@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sine-ai/n8n-nodes-jira-data-center.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
		"lint": "eslint nodes/**/*.ts credentials/**/*.ts",
    "lintfix": "eslint nodes/**/*.ts credentials/**/*.ts package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/JiraDataCenterApi.credentials.js",
      "dist/credentials/JiraDataCenterBasicAuth.credentials.js"
    ],
    "nodes": [
      "dist/nodes/JiraDataCenter/JiraDataCenter.node.js",
      "dist/nodes/JiraDataCenter/JiraDataCenterTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.12",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
