{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "lib": [
      "es2019"
    ],
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "credentials/**/*",
    "nodes/**/*"
  ],
  "exclude": [
    "./dist/**/*",
    "./node_modules/**/*"
  ]
}
